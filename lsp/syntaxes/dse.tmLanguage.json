{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "dse",
    "scopeName": "source.dse",
    "patterns": [{
            "include": "#keywords"
        },
        {
            "include": "#strings"
        }
    ],
    "repository": {
        "keywords": {
            "patterns": [{
                "name": "keyword.control.dse",
                "match": "(?<![\\w./-])(simulation|model|channel|uses|workflow|as|stack|network|var|arch|file|uid|envar|stepsize|endtime|user|token|stacked|sequential|path)(?![\\w./-])",
                "scope": "keyword.control"
            }]
        }
    }
}
