-- stack_fmu.yaml --
---
kind: Stack
metadata:
  name: stack
spec:
  connection:
    transport:
      redispubsub:
        uri: redis://localhost:6379
        timeout: 60
  models:
    - name: simbus
      model:
        name: simbus
      channels:
        - name: Foo
          expectedModelCount: 1
        - name: Bar
          expectedModelCount: 1

    - name: FMU1
      uid: 42
      model:
        name: FMU1
      channels:
        - name: Foo
          alias: signal_channel1
          selectors:
            model: FMU1
            channel: signal_vector1
    - name: FMU2
      uid: 41
      model:
        name: FMU2
      channels:
        - name: Bar
          alias: signal_channel2
          selectors:
            model: FMU2
            channel: signal_vector2

# INPUT model shall provide constant data for network signals ...
    - name: input
      uid: 101010
      model:
        name: Input
      channels:
        - name: Foo
          alias: input_channel
          selectors:
            channel: input_vector
        - name: Bar
          alias: input_channel
          selectors:
            channel: input_vector
