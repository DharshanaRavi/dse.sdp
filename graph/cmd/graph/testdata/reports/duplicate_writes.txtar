-- model__input.yaml --
---
kind: Model
metadata:
  name: Input
  annotations:
    reference: https://github.boschdevcloud.com/bios-emthacks/dse.input.git
    license: BIOSLv4
spec:
  runtime:
    dynlib:
      - os: windows
        arch: x64
        path: models/input/model/bin/input.dll
      - os: linux
        arch: amd64
        path: models/input/model/lib/input.so
  channels:
    - alias: input_channel
      selectors:
        model: input
        channel: input_vector


