{
	"compilerOptions": {
	  "module": "commonjs",
	  "target": "es2020",
	  "lib": ["es2020"],
	  "outDir": "out",
	  "rootDir": ".",
	  "baseUrl": ".",
	  "paths": {
		"*": ["lib/*", "bin/*"]
	  },
	  "sourceMap": true,
	  "strict": true,
	  "esModuleInterop": true,
	  "resolveJsonModule": true
	},
	"include": ["lib/**/*", "bin/**/*", "examples/scripts/*"],
	"exclude": ["node_modules", "out"]
  }
  