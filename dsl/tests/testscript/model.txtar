# Setup the environment.
env PATH=$ENTRYDIR/tests/scripts:$PATH

# Generate the AST.
exec parse2ast dsl_fmu.txt ast_fmu.json

# Evaluate the AST.
exec ast_stats.sh ast_fmu.json

stdout 'sims = 1'
stdout 'channels = 4'
stdout 'networks = 1'
stdout 'uses = 2'
stdout 'models = 1'
stdout 'stacks = 1'
stdout 'vars = 3'
stdout 'envar = 0'

# Evaluate the AST path/structure.
exec ast_paths.sh ast_fmu.json

